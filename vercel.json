{
    "builds": [
      {
        "src": "./build/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/build/index.js"
      }
    ],
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "access-control-allow-origin",
              "value": "*"
            }
          ]
        }
    ]
  }